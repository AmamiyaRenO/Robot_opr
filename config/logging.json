{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "format": "{\"ts\": \"%(asctime)s\", \"level\": \"%(levelname)s\", \"service\": \"%(name)s\", \"msg\": \"%(message)s\"}",
      "datefmt": "%Y-%m-%dT%H:%M:%S.%03Z"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  }
}

